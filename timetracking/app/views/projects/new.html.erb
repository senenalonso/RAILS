<h1>Add a project</h1>
<%= form_for @project, :html =>{:multipart => true} do |f| %>
	<%= f.label :name %><span>: </span>
	<%= f.text_field :name %>

	<%= f.label :description %><span>: </span>
	<%= f.text_area :description %>

	<br>
	
	<%= f.label :logo %><span>: </span>
	<%= f.file_field :logo %>

	<br>

	<%= f.button %>
<%end%>	